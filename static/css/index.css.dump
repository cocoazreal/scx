.device-status-list {
  list-style: none;
  padding-left: 40px;
  float: left;
}
.device-status-list>li {
  padding: 8px 0px;
}
.device-statue-block {
  position: absolute;
  left: 25px;
  width: 18px;
  height: 18px;
  border: 1px solid #aaa;
}
.communication-status-list {
  list-style: none;
  padding-left: 10px;
  float: right;
}
.communication-status-list>li {
  padding: 2px 5px;
  margin: 3px 0px;
  border: 1px solid #aaa;
}
