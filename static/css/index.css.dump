body{
  margin: 0px;
  padding: 0px;
}

.container{
  width: 1060px !important;
}

#nav{
  margin-bottom: 0px;
}

#btn-save{
  position: relative;
  right: 13px;
}

/*状态栏css*/
#status{
  padding-top: 60px;
  background: #eeeeee;
}
.stopped {
  background-color: #ccc;
}
.running {
  background-color: #0f0;
}
.warning {
  background-color: #ff0;
}
.fault {
  background-color: #f00;
}
.has-sheet {
  background-color: #000;
}
.no-sheet {
  background-color: #fff;
}
.device {
  position: absolute;
  padding: 5px 2px;
  text-align: center;
  display: table;
  border: 1px solid #aaa;
  /*产生带圆角的边框*/
  -webkit-border-radius: 5px;
  -moz-border-radius: 5px;
  border-radius: 5px;
}
.device>span {
  display: table-cell;
  vertical-align: middle;
}
.furnaces-door {
  position: absolute;
  top: 2px;
  left: 220px;
  width: 20px;
  height: 20px;
  border: 1px solid #aaa;
}
#unstack-robot,
#ffr-robot,
#fur-robot,
#pfr-robot,
#pur-robot {
  top: 60px;
  width: 80px;
  height: 60px;
}
#press {
  top: 25px;
  width: 80px;
  height: 130px;
}
#furnacesA,
#furnacesB,
#furnaces-doorA,
#furnaces-doorB {
  width: 255px;
  height: 55px;
}
#furnacesA {
  top: 125px;
}
#furnacesA>span {
  vertical-align: bottom;
}
#furnacesB {
  top: 0px;
}
#furnacesB>span {
  vertical-align: top;
}
#furnaces-doorA {
  top: 94px;
  height: 20px;
  padding: 2px;
}
#furnaces-doorB {
  top: 60px;
  height: 20px;
  padding: 2px;
}
